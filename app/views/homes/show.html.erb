<div class="container">
  <div class="row">
    <div class="text-warning">
      <div id="bookings-calendar" data-bookings="<%= @bookings_json %>"></div>
    </div>
    <div class="col-xs-12 col-md-4 col-lg-4">
        <div class="txt-form">
          <h1>Create a Booking</h1>
        </div>

        <%= render 'bookings/form', home: @home, booking: @booking %>
        <%= link_to "Edit this Home", edit_home_path(@home), class: "btn btn-outline-secondary" %>
      </div>
    </div>
</div>

<%= link_to "Edit this Home", edit_home_path(@home) %>

<div class="notes">
  <div class="comment-form">
    <%= simple_form_for ([@home, @note]), html: { class: "note-form" } do |f| %>
      <%= f.input :content, label: "Leave a comment:" %>
      <%= f.submit 'Create Note', class: "btn btn-gradient"%>
    <% end %>
  </div>
  <div class="comments-index">
    <% @home.notes.each do |note| %>
      <div class="note-cards">
        <p> <strong><%= note.user.name %>:</strong> <%= note.content %></p>
      </div>
        <%= link_to 'Delete', note_path(note), method: :delete, class: "btn btn-danger" %>
    <% end %>
  </div>
</div>

<div id="map"
     style="width: 100%; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>
